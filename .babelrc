{
  "presets": [
    ["env", {
      "targets": {
        "chrome": 52,
        "browsers": ["last 2 versions", "safari >= 7"]
      }
    }]
  ],
  "plugins": [
    "lodash",
    "add-module-exports",
    "transform-object-rest-spread",
    "transform-object-assign",
    "transform-export-extensions"
  ],
  "env": {
    "es": {
      "comments": false,
      "plugins": [
        "transform-inline-environment-variables"
      ]
    },
    "commonjs": {
      "comments": false,
      "plugins": [
        "transform-inline-environment-variables"
      ]
    },
    "test": {
      "plugins": [
        ["module-resolver", {
         "root": ["./src"]
       }]
      ]
    }
  }
}
